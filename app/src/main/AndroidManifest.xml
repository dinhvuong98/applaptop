<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.applaptop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.HomeActivity"
            android:label="Trang chủ" />
        <activity
            android:name=".activity.RegisterActivity"
            android:label="Đăng ký tài khoản" />
        <activity
            android:name=".activity.ForgotPassActivity"
            android:label="Xác thực tài khoản" />
        <activity
            android:name=".activity.ChangePassForgotActivity"
            android:label="Đổi mật khẩu" />
        <activity
            android:name=".activity.ProfileActivity"
            android:label="Tài khoản" />
        <activity
            android:name=".activity.ChangePass2Activity"
            android:label="Đổi mật khẩu" />
        <activity android:name=".Adapter.AdapterHome"/>
        <activity android:name=".activity.DellActivity"
            android:label="Sản phẩm dell"></activity>
        <activity android:name=".activity.AsusActivity"
            android:label="sản phẩm asus"/>
        <activity android:name=".activity.DetailProductAvtivity"
            android:label="Chi Tiết Sản Phẩm"></activity>
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
</manifest>